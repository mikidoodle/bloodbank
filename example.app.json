{
    "expo": {
        "name": "JIPMER",
        "slug": "bloodbank",
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "bloodbank",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "icon": "./assets/images/icon.png",
            "supportsTablet": true,
            "bundleIdentifier": "",
            "config": {
                "usesNonExemptEncryption": false
            },
            "infoPlist": {
                "com.apple.developer.usernotifications.critical-alerts": 1
            },
            "entitlements": {
                "com.apple.developer.usernotifications.critical-alerts": true
            },
            "splash": {
                "image": "./assets/images/splash.png",
                "resizeMode": "contain",
                "backgroundColor": "#ffffff",
                "dark": {
                    "image": "./assets/images/splash.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#000000"
                }
            }
        },
        "android": {
            "icon": "./assets/images/icon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#EFEEF7"
            },
            "permissions": [
                "android.permission.ACCESS_COARSE_LOCATION",
                "android.permission.ACCESS_FINE_LOCATION",
                "android.permission.CAMERA",
                "android.permission.RECORD_AUDIO"
            ],
            "splash": {
                "image": "./assets/images/splash.png",
                "resizeMode": "contain",
                "backgroundColor": "#ffffff",
                "dark": {
                    "image": "./assets/images/splash.png",
                    "resizeMode": "contain",
                    "backgroundColor": "#000000"
                }
            },
            "package": "",
            "googleServicesFile": "./google-services.json",
            "config": {
                "googleMaps": {
                    "apiKey": ""
                }
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-notifications",
                {
                    "icon": "./assets/images/icon.png",
                    "color": "#ffffff",
                    "defaultChannel": "default"
                }
            ],
            [
                "expo-location",
                {
                    "locationWhenInUsePermission": "$(PRODUCT_NAME) requires location access to prioritize nearby donors during urgent blood needs.",
                    "alwaysLocationPermission": "$(PRODUCT_NAME) requires location access to prioritize nearby donors during urgent blood needs."
                }
            ],
            [
                "expo-camera",
                {
                    "cameraPermission": "$(PRODUCT_NAME) requires the camera to scan donor codes",
                    "microphonePermission": "$(PRODUCT_NAME) requires access to the microphone to record audio",
                    "recordAudioAndroid": true
                }
            ],
            "expo-router",
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": ""
            }
        }
    }
}
